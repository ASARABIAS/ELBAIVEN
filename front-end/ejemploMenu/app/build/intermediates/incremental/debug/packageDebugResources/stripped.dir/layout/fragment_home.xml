<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="#E4E1E1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/button_beer"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_marginTop="16dp"
            android:adjustViewBounds="false"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_liqueurs"
            android:text="@string/next"
            app:layout_constraintEnd_toStartOf="@+id/button_liqueur"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button_liqueur"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_marginTop="16dp"
            android:adjustViewBounds="false"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_liqueurs"
            android:text="@string/next"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button_beer"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button_soda"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="16dp"
            android:adjustViewBounds="false"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_liqueurs"
            android:text="@string/next"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button_water"
            app:layout_constraintTop_toBottomOf="@+id/button_liqueur"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button_water"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="16dp"
            android:adjustViewBounds="false"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_liqueurs"
            android:text="@string/next"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/button_soda"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_beer"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="126dp"
            android:layout_height="28dp"
            android:layout_marginStart="12dp"
            android:layout_marginBottom="12dp"
            android:autoText="false"
            android:background="#FFFFFF"
            android:fontFamily="monospace"
            android:layerType="none"
            android:text="@string/beer"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:circularflow_angles=" "
            app:layout_constraintBottom_toBottomOf="@+id/button_beer"
            app:layout_constraintStart_toStartOf="@+id/button_beer" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="126dp"
            android:layout_height="28dp"
            android:layout_marginStart="12dp"
            android:layout_marginBottom="12dp"
            android:autoText="false"
            android:background="#FFFFFF"
            android:fontFamily="monospace"
            android:layerType="none"
            android:text="@string/liqueurs"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:circularflow_angles=" "
            app:layout_constraintBottom_toBottomOf="@+id/button_liqueur"
            app:layout_constraintStart_toStartOf="@+id/button_liqueur" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="126dp"
            android:layout_height="28dp"
            android:layout_marginStart="12dp"
            android:layout_marginBottom="12dp"
            android:autoText="false"
            android:background="#FFFFFF"
            android:fontFamily="monospace"
            android:layerType="none"
            android:text="@string/soda"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:circularflow_angles=" "
            app:layout_constraintBottom_toBottomOf="@+id/button_soda"
            app:layout_constraintStart_toStartOf="@+id/button_soda" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="126dp"
            android:layout_height="28dp"
            android:layout_marginStart="12dp"
            android:layout_marginBottom="12dp"
            android:autoText="false"
            android:background="#FFFFFF"
            android:fontFamily="monospace"
            android:layerType="none"
            android:text="@string/water"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:circularflow_angles=" "
            app:layout_constraintBottom_toBottomOf="@+id/button_water"
            app:layout_constraintStart_toStartOf="@+id/button_water" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>